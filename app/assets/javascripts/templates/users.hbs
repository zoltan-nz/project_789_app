<table class='table'>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>E-mail</th>
            <th>Admin?</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            {{#each users}}
                <td>{{first_name}}</td>
                <td>{{last_name}}</td>
                <td>{{user.email}}</td>
                <td>{{admin}}</td>
            {{/each}}
        </tr>
    </tbody>
</table>